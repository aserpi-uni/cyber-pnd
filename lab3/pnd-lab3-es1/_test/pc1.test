#!/bin/bash

if nslookup web.pndeflab.edu > /dev/null; then
    echo "DNS ok"
else
    echo "DNS failure"
fi

if wget -t 1 -T 1 100.64.6.2 -q; then
    echo "Internal web failure"
else
    echo "Internal web ok"
fi

wget -t 1 -T 1 google.com -q
if [ $? = 0 ] ; then
    echo "Internet ok"
else
    echo "Internet failure"
fi
